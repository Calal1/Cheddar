{
    "presets": [
        ["env", {
            "targets": {
                "node": 4
            }
        }]
    ],
    "plugins": [
        "babel-plugin-transform-class-properties",
        "babel-plugin-add-module-exports",
        "babel-plugin-compile-cheddar",
        "babel-plugin-tailcall-optimization",
        "babel-plugin-loop-optimizer"
    ],
    "env": {
        "production": {
            "plugins": [
                "babel-plugin-strip-checks"
            ]
        }
    }
}
